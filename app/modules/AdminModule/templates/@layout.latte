<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{ifset title}{include title|stripHtml} | {/ifset}Administration</title>

        <!--STYLE Bootstrap stylesheet-->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
        <!--STYLE Font general-->
        <link href="https://fonts.googleapis.com/css?family=Encode+Sans+Condensed:300,400,500,600,700&amp;subset=latin-ext" rel="stylesheet">
        <!--STYLE Farbtastic colorPicker-->
        <link rel="stylesheet" href="{$basePath}/admin/js/farbtastic/farbtastic.css" type="text/css" />
        <!--CSS file upload-->
        <link href="{$basePath}/admin/js/fileinput/css/fileinput.min.css" media="all" rel="stylesheet" type="text/css" />
        <!--STYLE Bootstrap dateTimePicker-->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.47/css/bootstrap-datetimepicker.min.css" />
        <!--STYLE Custom stylesheet-->
        <link href="{$basePath}/admin/css/style.css" rel="stylesheet" >

        <!--SCRIPT jQuery-->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
        <!--SCRIPT Tooltip-->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
        <!--SCRIPT Bootstrap-->
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <!--SCRIPT same height panel-->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.matchHeight/0.7.2/jquery.matchHeight-min.js"></script>
        <!-- Bootstrap Date-Picker Plugin -->
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.21.0/moment.min.js"></script>
        <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.47/js/bootstrap-datetimepicker.min.js"></script>
        <!--SCRIPT Farbtastic colorPicker-->
        <script type="text/javascript" src="{$basePath}/admin/js/farbtastic/farbtastic.js"></script>
        <!--SCRIPT tinyMCE textEditor-->
        <script src="{$basePath}/admin/js/tinymce/tinymce.min.js"></script>
        <!--SCRIPT fileinput fetures-->
        <script src="{$basePath}/admin/js/fileinput/js/plugins/piexif.min.js" type="text/javascript"></script>
        <script src="{$basePath}/admin/js/fileinput/js/plugins/purify.min.js" type="text/javascript"></script>
        <!--SCRIPT filepicker-->
        <script src="{$basePath}/admin/js/fileinput/js/fileinput.min.js"></script>
        <script src="{$basePath}/admin/js/fileinput/themes/fa/theme.js"></script>
        <script src="{$basePath}/admin/js/fileinput/js/locales/LANG.js"></script>
        <!--SCRIPT nette ajax-->
        <script src="{$basePath}/admin/js/nette.ajax.js" type="text/javascript"></script>
        <script src="{$basePath}/admin//js/main.js" type="text/javascript"></script>
        <!--SCRIPT custom scripts-->
        <script src="{$basePath}/admin/js/js.js"></script>

    </head>

    <body>

        <div id="flashMessage" class="divFlashMessage">
            <ul class="ulFlashMessage">
            </ul>
        </div>


            <nav class="navbar navbar-default visible-xs">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                    </div>
                    <div class="collapse navbar-collapse" id="myNavbar">
                        <ul class="nav navbar-nav ulmenu">
                            <li id="home"><a n:href="Main:" >Home</a></li>
                            <li id="summary"><a n:href="Summary:" >Manage</a></li>
                            <li><a n:href=":Front:Homepage:" target="_blank">Web</a></li>
                        </ul>
                    </div>
                </div>
            </nav>
            <!--menu pro velkÃ© obrazovky-->
            <div class="col-md-2 divLayoutLeft hidden-xs">
                <a n:href="Main:"><img class="img-responsive imgLogo" src="{$basePath}/admin/img/svg/logo.svg" alt="logo"/></a>
                <div class="divMenu">
                    <ul class="menu">
                        <li id="home"><a n:href="Main:" >Home</a></li>
                        <li id="summary"><a n:href="Summary:" >Manage</a></li>
                        <li id="summary"><a n:href="Menu:edit" >Menu</a></li>
                        <li id="summary"><a n:href="Seo:edit" >SEO</a></li>
                        <li><a n:href=":Front:Homepage:" target="_blank">Web</a></li>
                    </ul>
                </div>
            </div>






            <div class="divLayoutTop navbar hidden-xs">
                <div class="divPageH">
                    {include heading}
                </div>
                <ul class="list-inline ulNav">
                    <li><i class="fa fa-user-circle-o" aria-hidden="true"></i></li>
                    <li>{ifset $this->getParameter('user')->getIdentity()->data['email']}{$this->getParameter('user')->getIdentity()->data['email']}{/ifset}</li>
                    <li>|</li>
                    <li><a class="bila" n:href="logout"><strong>Sign out</strong></a></li>
                </ul>
            </div>



            <div class="divLayoutRight by-height">
                {include content}
            </div>



    {block css}
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    {/block}



    <script type="text/javascript">
        {foreach $flashes as $flash}
            var flash = {$flash->message}
            flashMessage(flash);
        {/foreach}
    </script>
    {ifset bordel}{include bordel}{/ifset}
    {block scripts}
        <script src="https://nette.github.io/resources/js/netteForms.min.js"></script>
    {/block}
    </body>
</html>
